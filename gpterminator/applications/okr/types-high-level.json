            },
            {
                "name": "Owner",
                "type": "Reference",
                "multiplicity": "one, mandatory",
                "description": "The individual responsible for the objective."
            }
        ]
    },
    {
        "name": "Key Result",
        "attributes": [
            {
                "name": "Title",
                "type": "Text",
                "multiplicity": "one, mandatory",
                "description": "Title of the key result."
            },
            {
                "name": "Description",
                "type": "Text",
                "multiplicity": "one, optional",
                "description": "Detailed description of the key result."
            },
            {
                "name": "Objective",
                "type": "Reference",
                "multiplicity": "one, mandatory",
                "description": "The objective this key result belongs to."
            },
            {
                "name": "TargetValue",
                "type": "Number",
                "multiplicity": "one, mandatory",
                "description": "The target value to be achieved."
            },
            {
                "name": "CurrentValue",
                "type": "Number",
                "multiplicity": "one, optional",
                "description": "The current progress value."
            }
        ]
    }
