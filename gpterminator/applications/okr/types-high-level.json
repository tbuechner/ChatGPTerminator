[
    {
        "name": "Cycle",
        "attributes": [
            {
                "name": "Name",
                "type": "Text",
                "multiplicity": "one, mandatory",
                "description": "The name of the cycle (e.g., Q1 2023)."
            },
            {
                "name": "StartDate",
                "type": "Date",
                "multiplicity": "one, mandatory",
                "description": "The start date of the cycle."
            },
            {
                "name": "EndDate",
                "type": "Date",
                "multiplicity": "one, mandatory",
                "description": "The end date of the cycle."
            },
            {
                "name": "Objectives",
                "type": "Reference",
                "multiplicity": "many, optional",
                "description": "The set of objectives pursued during the cycle."
            }
        ]
    },
    {
        "name": "Objective",
        "attributes": [
            {
                "name": "Title",
                "type": "Text",
                "multiplicity": "one, mandatory",
                "description": "Title of the objective."
            },
            {
                "name": "Description",
                "type": "Text",
                "multiplicity": "one, optional",
                "description": "Detailed description of the objective."
            },
            {
                "name": "Cycle",
                "type": "Reference",
                "multiplicity": "one, mandatory",
                "description": "The cycle during which the objective is pursued."
            },
            {
                "name": "KeyResults",
                "type": "Reference",
                "multiplicity": "many, optional",
